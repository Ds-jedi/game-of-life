node 
{
    stage('scm')
    {
        git 'https://github.com/Ds-jedi/game-of-life'
    }
    stage('build')
    {
        sh 'mvn package'
    }
    stage('Sonar') 
    {
        withSonarQubeEnv('sonar') 
        {
            sh 'mvn org.sonarsource.scanner.maven:sonar-mavenplugin:3.6.0.1398:sonar'
        }
    }
}
